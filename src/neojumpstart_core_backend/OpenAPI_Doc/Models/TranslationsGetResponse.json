{
    "type": "object",
    "properties": {
        "statusCode": {
            "type": "integer",
            "description": "Status code of the response (200)"
        },
        "body": {
            "type": "object",
            "description": "Body of the response",
            "properties": {
                "language": {
                    "type": "object",
                    "description": "language initials of the translation",
                    "properties": {
                        "translation": {
                            "type": "object",
                            "description": "dictionary that contains the components for the languaje in that tenant",
                            "properties": {
                                "general": {
                                    "type": "object",
                                    "description": "dictionary that contains the translations for that languaje in that component",
                                    "properties": {
                                        "custom translation name": {
                                            "type": "object",
                                            "description": "dictionary that contains the translation name and the custom translation",
                                            "properties": {
                                                "custom translation value": {
                                                    "type": "string",
                                                    "description": "custom translation"
                                                }
                                            }
                                        }
                                    }
                                }
                            }

                        }
                    }
                },
                "UUID": {
                    "type": "string",
                    "description": "Unique ID of the execution"
                }
            }
        },
        "headers": {
            "type": "object",
            "description": "Object containing CORS Headers",
            "properties": {
                "Access-Control-Allow-Origin": {
                    "type": "string",
                    "description": "*"
                },
                "Access-Control-Allow-Credentials": {
                    "type": "string",
                    "description": "true"
                },
                "Access-Control-Allow-Methods": {
                    "type": "string",
                    "description": "GET,HEAD,OPTIONS,POST,PUT"
                },
                "Access-Control-Allow-Headers": {
                    "type": "string",
                    "description": "Access-Control-Allow-Headers, Origin,Accept, X-Requested-With, Content-Type, Access-Control-Request-Method, Access-Control-Request-Headers"
                }
            }
        }
    }
}