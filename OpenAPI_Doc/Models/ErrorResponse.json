{
    "type": "object",
    "properties": {
        "statusCode": {
            "type": "integer",
            "description": "Status code of the response (500)"
        },
        "body": {
            "type": "object",
            "description": "Body of the response",
            "properties": {
                "message": {
                    "type": "string",
                    "description": "Error message"
                },
                "code": {
                    "type": "string",
                    "description": "Error code"
                },
                "UUID": {
                    "type": "string",
                    "description": "Unique ID of the execution"
                }
            }
        },
        "headers": {
            "type": "object",
            "description": "Object containing CORS Headers",
            "properties": {
                "Access-Control-Allow-Origin": {
                    "type": "string",
                    "description": "*"
                },
                "Access-Control-Allow-Credentials": {
                    "type": "string",
                    "description": "true"
                },
                "Access-Control-Allow-Methods": {
                    "type": "string",
                    "description": "GET,HEAD,OPTIONS,POST,PUT"
                },
                "Access-Control-Allow-Headers": {
                    "type": "string",
                    "description": "Access-Control-Allow-Headers, Origin,Accept, X-Requested-With, Content-Type, Access-Control-Request-Method, Access-Control-Request-Headers"
                }
            }
        }
    }
}